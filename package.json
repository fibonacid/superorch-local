{
  "name": "colliderchat-prototype",
  "version": "0.0.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/lorenzorivosecchi/colliderchat-prototype.git"
  },
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "dev": "NODE_ENV='development' nf start -p 3000",
    "build": "react-scripts build",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "package": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never",
    "deploy": "electron-builder build --mac --config=electron-builder.yml -c.extraMetadata.main=build/electron/main.js --publish always",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "node server/app.js"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "resolutions": {
    "**/**/fsevents": "^1.2.9"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^21.1.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-updater": "^4.2.0",
    "foreman": "^2.0.0",
    "react-scripts": "^3.2.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "dependencies": {
    "@giantmachines/redux-websocket": "^1.1.5",
    "chance": "^1.1.3",
    "dotenv": "^8.2.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "styled-components": "^4.4.0",
    "styled-normalize": "^8.0.6",
    "styled-reset": "^4.0.3",
    "ws": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
