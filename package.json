{
  "name": "superorch",
  "version": "v0.1.4",
  "description": "A supercollider IDE for laptop orchestras",
  "private": true,
  "homepage": "./",
  "main": "src/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lorenzorivosecchi/superorch.git"
  },
  "scripts": {
    "dev": "NODE_ENV='development' nf start -p 3000",
    "start:server": "webpack --watch --config server.config.js",
    "start:react": "export BROWSER=none && react-scripts start",
    "start:electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build:react": "react-scripts build",
    "build:server": "webpack --config server.config.js",
    "build:electron": "mkdir build/src && cp -r src/electron/. build/src/electron && cp -r src/shared/. build/src/shared",
    "package:mac": "electron-builder build --mac -c.extraMetadata.main=build/src/electron/main.js --publish never",
    "package:win": "electron-builder build --win -c.extraMetadata.main=build/src/electron/main.js --publish never",
    "package:linux": "electron-builder build --linux -c.extraMetadata.main=build/src/electron/main.js --publish never",
    "package": "electron-builder build -c.extraMetadata.main=build/src/electron/main.js --publish never",
    "deploy": "electron-builder build --config=electron-builder.yml -c.extraMetadata.main=build/src/electron/main.js --publish always",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:update": "react-scripts test --env=jsdom --updateSnapshot",
    "eject": "react-scripts eject"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.1.1",
    "electron-devtools-installer": "^2.2.4",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "express-http-proxy": "^1.6.0",
    "foreman": "^3.0.1",
    "husky": "^3.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^6.3.3",
    "nodemon-webpack-plugin": "^4.2.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^3.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "redux-test-utils": "^0.3.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@giantmachines/redux-websocket": "^1.1.5",
    "babel-polyfill": "^6.26.0",
    "chance": "^1.1.3",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.0",
    "draftjs-utils": "^0.9.4",
    "electron-updater": "^4.2.0",
    "enzyme": "^3.10.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "gsap": "^3.0.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "styled-components": "^4.4.0",
    "styled-normalize": "^8.0.6",
    "styled-reset": "^4.0.3",
    "supercolliderjs": "^1.0.0-beta.1",
    "ws": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engineStrict": false,
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "electron": "<rootDir>/src/react/mocks/electron.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
