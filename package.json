{
  "name": "colliderchat-client",
  "version": "0.0.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lorenzorivosecchi/colliderchat-client.git"
  },
  "scripts": {
    "start": "export BROWSER=none && react-app-rewired start",
    "start:electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "dev": "NODE_ENV='development' nf start -p 3000",
    "build": "react-app-rewired build",
    "build:electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "package": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never",
    "deploy": "electron-builder build --mac --config=electron-builder.yml -c.extraMetadata.main=build/electron/main.js --publish always",
    "test": "react-app-rewired test --env=jsdom",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:update": "react-app-rewired test --env=jsdom --updateSnapshot",
    "eject": "react-scripts eject",
    "server": "node server/app.js"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.1.1",
    "electron-devtools-installer": "^2.2.4",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "foreman": "^3.0.1",
    "husky": "^3.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-app-rewired": "^2.1.4",
    "react-scripts": "^3.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "redux-test-utils": "^0.3.0"
  },
  "dependencies": {
    "@giantmachines/redux-websocket": "^1.1.5",
    "chance": "^1.1.3",
    "diff": "^4.0.1",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.0",
    "electron-updater": "^4.2.0",
    "enzyme": "^3.10.0",
    "gsap": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "styled-components": "^4.4.0",
    "styled-normalize": "^8.0.6",
    "styled-reset": "^4.0.3",
    "supercolliderjs": "^1.0.0-beta.1",
    "ws": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engineStrict": false,
  "engines": {
    "node": ">=12.13.0",
    "yarn": ">=1.19.1"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
